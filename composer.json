    "type": "library",
    "description": "for create pdf file from html",
    "homepage": "https://github.com/ali-nikookherad/pdf",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "laravel/framework": "^9.19",
        "guzzlehttp/guzzle": "~5.0"
    },
